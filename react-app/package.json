{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "4.5.0",
        "@kbase/ui-components": "0.2.1-alpha.22",
        "@kbase/ui-lib": "0.2.1-alpha.3",
        "@testing-library/jest-dom": "5.11.9",
        "@testing-library/react": "11.2.5",
        "@types/jest": "26",
        "@types/marked": "1",
        "@types/node": "14",
        "@types/react": "16",
        "@types/react-dom": "16",
        "@types/react-redux": "7",
        "@types/testing-library__jest-dom": "5.9.5",
        "@types/testing-library__react": "10.2.0",
        "antd": "4.12.3",
        "antd-dayjs-webpack-plugin": "1.0.6",
        "babel-plugin-import": "1.13.3",
        "customize-cra": "1.0.0",
        "http-proxy-middleware": "1.0.6",
        "less": "4.1.1",
        "less-loader": "7.3.0",
        "marked": "1.2.9",
        "morgan": "1.10.0",
        "nodemon": "2.0.7",
        "react": "16.14.0",
        "react-app-rewired": "2.1.8",
        "react-dom": "16.14.0",
        "react-redux": "7.2.2",
        "react-scripts": "4.0.3",
        "redux": "4.0.5",
        "redux-thunk": "2.3.0",
        "typescript": "4.2.2",
        "vis-data": "7.1.2",
        "vis-network": "9.0.2"
    },
    "scripts": {
        "start": "nodemon -w ./config-overrides.js --exec 'react-app-rewired start'",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "test-coverage": "react-app-rewired test --coverage --watchAll=false"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!**/*.js"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}
