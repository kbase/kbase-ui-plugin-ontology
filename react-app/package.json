{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "4.7.0",
        "@kbase/ui-components": "0.5.17",
        "@kbase/ui-lib": "0.4.12",
        "@types/marked": "^3.0.2",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@types/react-redux": "^7.1.20",
        "@types/react-router-dom": "^5.3.2",
        "antd": "4.16.13",
        "antd-dayjs-webpack-plugin": "1.0.6",
        "less": "4.1.2",
        "less-loader": "7.3.0",
        "marked": "4.0.10",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-redux": "7.2.6",
        "react-router-dom": "5.3.0",
        "redux": "4.1.1",
        "redux-thunk": "2.4.0",
        "vis-data": "7.1.2",
        "vis-network": "9.1.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "12.1.2",
        "@types/jest": "27",
        "@types/node": "^14.17.32",
        "@types/testing-library__jest-dom": "5",
        "babel-plugin-import": "1.13.3",
        "customize-cra": "1.0.0",
        "http-proxy-middleware": "2.0.1",
        "morgan": "1.10.0",
        "nodemon": "2.0.14",
        "react-app-rewired": "2.1.8",
        "react-scripts": "4.0.3",
        "typescript": "4.4.4"
    },
    "scripts": {
        "start": "nodemon -w ./config-overrides.js --exec 'react-app-rewired start'",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "test-coverage": "react-app-rewired test --coverage --watchAll=false"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!**/*.js"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}
